express = require 'express'
responsive = require '../../../responsive'
app = express()

app.configure ->
	app.set 'views', "#{__dirname}/views"
	app.set 'view engine', 'jade'

	app.enableDeviceRouting()


app.get '/', (req, res) ->
	res.render 'test'
app.get '/fallback', (req, res) ->
	res.render 'test2'

module.exports = app